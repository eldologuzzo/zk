    apply="com.tpvs.tpvsadmin.zkutils.TerminalsPopUpController">
        <hbox>
            <groupbox id="gb10" mold="3d" width="440px">
                <caption label="${c:l('IngreseDatos')}:"/>
                <!--<hbox>
                    <vbox width="100px">Id Grupo:</vbox>
                    <label id="idGroup" value="@{win$composer.current.idGroup}" />
                </hbox>
                <space/>-->
                <hbox>
                    <vbox width="150px">Id:</vbox>
                    <label id="idTerminalLbl" visible="false"/>
                    <textbox id="idTerminal" cols="22" visible="false" value="@{windowpopup1$composer.current.idTerminal}"/>
                </hbox>
                <space/>
                <hbox>
                    <vbox width="150px">${c:l('Modelo')}:</vbox>
                    <textbox id="model" cols="8" maxlength="10" value="@{windowpopup1$composer.current.model}"/>
                </hbox>
                <space/>
                <hbox>
                    <vbox width="150px">${c:l('LicenciaBrowser')}:</vbox>
                    <textbox id="browserLic" cols="42" value="@{windowpopup1$composer.current.browserLic}"/>
