                                        <textbox id="offLineResponseCodesApproved" style="font-size:100%;" width="100%" value="@{win$composer.currentIcsemv.offLineResponseCodesApproved}" maxlength="2"/>
                                    </row>
                                    <row>
                                        <label multiline="true" style="font-size:100%;">
