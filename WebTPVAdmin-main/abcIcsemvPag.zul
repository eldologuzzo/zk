                                        <checkbox id="easyEntryAllowed" label="easyEntryAllowed" style="font-size:100%;" checked="@{win$composer.currentIcsemv.easyEntryAllowed}"/>
                                        <checkbox id="goOnlineIfAar" label="goOnlineIfAar" style="font-size:100%;" checked="@{win$composer.currentIcsemv.goOnlineIfAar}"/>
                                        <checkbox id="specialProcessingArqc" label="specialProcessingArqc" style="font-size:100%;" checked="@{win$composer.currentIcsemv.specialProcessingArqc}"/>
                                        <checkbox id="userIssuerFlootLimit" label="userIssuerFlootLimit" style="font-size:100%;" checked="@{win$composer.currentIcsemv.userIssuerFlootLimit}"/>
                                        <checkbox id="selectAidIfExactMatch" label="selectAidIfExactMatch" style="font-size:100%;" checked="@{win$composer.currentIcsemv.selectAidIfExactMatch}"/>
                                    </row>
