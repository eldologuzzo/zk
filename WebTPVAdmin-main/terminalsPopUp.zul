                </hbox>
                <space/>
                <hbox>
                    <vbox width="150px">${c:l('FechaRegistro')}:</vbox>
                    <datebox id="addDate" format="dd/MM/yyyy" cols="10" value="@{windowpopup1$composer.current.addDate}"/>
                </hbox>
                <space/>
                <hbox>
                    <vbox width="150px">${c:l('UltimaComunicacion')}:</vbox>
                    <label id="lastCommDate" value="@{windowpopup1$composer.current.lastCommDate}"/>
                </hbox>
                <space/>
                <hbox>
                    <vbox width="150px">${c:l('folio')}:</vbox>
                    <textbox id="folio" value="@{windowpopup1$composer.current.folio}"/>
                </hbox>
                <space/>
                <space/>
                <hbox>
                    <button id="bttnTermPopUp" label="Agregar" image="/images/plus-circle.png"/>
                    <button id="CancelTermPopUp" label="${c:l('BttnCerrar')}" image="/images/cross-circle.png" onClick="windowpopup1.detach();"/>
                    <button id="variables" label="${c:l('BttnVariables')}" disabled="true" image="/images/application-detail.png"/>

<!--                    <button id="vargfbanorte" label="VarsGFBanorte"
